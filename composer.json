{
  "name": "joby/bracketeer",
  "description": "A hybrid markup language that combines Markdown, wiki-style link tags, and its own easily extensible tag syntax for intuitive plaintext web authoring.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Joby\\Bracketeer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Joby\\Bracketeer\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Joby Elliott",
      "email": "code@byjoby.com"
    }
  ],
  "require": {
    "php": "~8.3",
    "league/commonmark": "~2.7",
    "symfony/yaml": "~7.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.2",
    "phpstan/phpstan": "^2.1"
  }
}
